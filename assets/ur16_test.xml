                                </body>
                            </body>
                        </body>
                    </body>
                </body>
            </body>
        </body>        
    
        <geom condim="6" pos="0.15 0 -0.25" size="4.0 4.0 0.05" friction="1.0 0.005 0.0001" rgba="0 1 0 1" type="box"/>

