<mujoco model="ur3e_robot">
    <compiler angle="radian" />
    <size njmax="500" nconmax="100" />
    <asset>
        <mesh name="ur16_base" file="ur16e/base.stl" />
        <mesh name="ur16_shoulder" file="ur16e/shoulder.stl" />
        <mesh name="ur16_upperarm" file="ur16e/upperarm.stl" />
        <mesh name="ur16_forearm" file="ur16e/forearm.stl" />
        <mesh name="ur16_wrist1" file="ur16e/wrist1.stl" />
        <mesh name="ur16_wrist2" file="ur16e/wrist2.stl" />
        <mesh name="ur16_wrist3" file="ur16e/wrist3.stl" />
        <mesh name="oct_nozzle" file="meshes/nozzle_convex_primitive.STL"/>
    </asset>
    <option 
        timestep="0.005" integrator="RK4">
    </option>
	<actuator>
		<motor gear="1.0" joint="ur16_shoulder_pan_joint" name="ur16_shoulder_pan_joint" ctrllimited="true" ctrlrange="-100 100"/>
		<motor gear="1.0" joint="ur16_shoulder_lift_joint" name="ur16_shoulder_lift_joint" ctrllimited="true" ctrlrange="-100 100"/>
		<motor gear="1.0" joint="ur16_elbow_joint" name="ur16_elbow_joint" ctrllimited="true" ctrlrange="-100 100"/>
		<motor gear="1.0" joint="ur16_wrist_1_joint" name="ur16_wrist_1_joint" ctrllimited="true" ctrlrange="-100 100"/>
        <motor gear="1.0" joint="ur16_wrist_2_joint" name="ur16_wrist_2_joint" ctrllimited="true" ctrlrange="-100 100"/>
        <motor gear="1.0" joint="ur16_wrist_3_joint" name="ur16_wrist_3_joint" ctrllimited="true" ctrlrange="-100 100"/>
        <!-- <motor gear="1.0" joint="joint:2" name="bact"/> -->
	</actuator>    
    <sensor>
        <force name='force_sensor' site='force_sensor'/>
        <torque name='torque_sensor' site='torque_sensor'/>
        <accelerometer name='acc' site='acc'/>
        <!-- <gyro name='gyro' site='gyro'/> -->
    </sensor>
    <worldbody>
