        <motor gear="1.0" joint="ur16_wrist_3_joint" name="ur16_wrist_3_joint" ctrllimited="true" ctrlrange="-100 100"/>
        <!-- <motor gear="1.0" joint="joint:2" name="bact"/> -->
	</actuator>    
