                                <!-- <joint name="wrist_3_joint" pos="0 0 0" axis="0 0 1" limited="true" range="-1.588058798181077 -1.57" /> -->
                                <joint name="wrist_3_joint" pos="0 0 0" axis="0 0 1" limited="true" range="-6.28319 6.28319" />
                                <geom pos="0 0 -0.0921" quat="0.707107 0.707107 0 0" type="mesh" rgba="0.7 0.7 0.7 1" mesh="wrist3" name="wrist3" />
                                
                                <body name="peg_base" pos="0 0 0.0025" >
                                    <geom pos="0.0 0 0.0"  size="0.03 0.0025" type="cylinder" rgba="0.7 0.7 0.7 1" name="pegbase" />
                                    <body name="peg" pos="0 0 0.0" >
                                        <geom pos="0.0 0 0.0025"  size="0.03 0.0025" type="cylinder" rgba="0.7 0.7 0.7 1" name="peg0" />   
                                        <!-- <geom pos="0.03 0 0.01"  size="0.01 0.01" type="cylinder" rgba="0.7 0.7 0.7 1" name="peg0" />
                                        <geom pos="-0.03 0 0.01"  size="0.01 0.01" type="cylinder" rgba="0.7 0.7 0.7 1" name="peg1"/> -->
                                        <!-- <site name='acc' pos="0 0 0.0" size='0.01 0.01 0.01' rgba='0 1 0 1' type='sphere' group='1'/>
                                        <site name='gyro' pos="0 0 0.0" size='0.01 0.01 0.01' rgba='0 1 0 1' type='sphere' group='1'/> -->
                                        <inertial pos = "0. 0. 0." mass="0.002"/>
                                        <!-- <body name="pegtip" pos="0 0 0.060" > -->
                                        <site name='force_sensor' pos="0 0 0.00" size='0.01 0.01 0.01' rgba='0 1 0 0' type='sphere' group='1'/>
                                        <site name='torque_sensor' pos="0 0 0.00" size='0.01 0.01 0.01' rgba='0 1 0 0' type='sphere' group='1'/>
                                        <site name='mep:ee' pos="0 0.0 -0.05" size='0.01 0.01 0.01' rgba='0 1 1 1' type='sphere' group='1'/>
                                        <site name='acc' pos="0 0 0.0" size='0.01 0.01 0.01' rgba='0 1 0 0' type='sphere' group='1'/>
                                    </body>
                                </body>                                
                            </body>
                        </body>
                    </body>
                </body>
            </body>
        </body>
        
        <!-- <site name='wpt1' pos="0.02526912 0.44041631 0.3" size='0.015 0.015 0.015' rgba='0 0 1 1' type='sphere' group='1' quat="-0.65328148 0.27059805007309845 0.6532814824381883 0.27059805007309845"/>  -->
