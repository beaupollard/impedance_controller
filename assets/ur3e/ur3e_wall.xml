<mujoco model="ur3e_robot">
    <compiler angle="radian" />
    <size njmax="500" nconmax="100" />
    <asset>
        <mesh name="base" file="base.stl" />
        <mesh name="shoulder" file="shoulder.stl" />
        <mesh name="upperarm" file="upperarm.stl" />
        <mesh name="forearm" file="forearm.stl" />
        <mesh name="wrist1" file="wrist1.stl" />
        <mesh name="wrist2" file="wrist2.stl" />
        <mesh name="wrist3" file="wrist3.stl" />
    </asset>
    <option 
        timestep="0.005" integrator="RK4">
    </option>
	<actuator>
		<motor gear="1.0" joint="shoulder_pan_joint" name="shoulder_pan_joint" ctrllimited="true" ctrlrange="-100 100"/>
		<motor gear="1.0" joint="shoulder_lift_joint" name="shoulder_lift_joint" ctrllimited="true" ctrlrange="-100 100"/>
		<motor gear="1.0" joint="elbow_joint" name="elbow_joint" ctrllimited="true" ctrlrange="-100 100"/>
		<motor gear="1.0" joint="wrist_1_joint" name="wrist_1_joint" ctrllimited="true" ctrlrange="-100 100"/>
        <motor gear="1.0" joint="wrist_2_joint" name="wrist_2_joint" ctrllimited="true" ctrlrange="-100 100"/>
        <motor gear="1.0" joint="wrist_3_joint" name="wrist_3_joint" ctrllimited="true" ctrlrange="-100 100"/>
        <!-- <motor gear="1.0" joint="joint:2" name="bact"/> -->
	</actuator>    
    <sensor>
        <force name='force_sensor' site='force_sensor'/>
        <torque name='torque_sensor' site='torque_sensor'/>
        <accelerometer name='acc' site='acc'/>
        <!-- <gyro name='gyro' site='gyro'/> -->
    </sensor>
    <worldbody>
    <!-- <camera name="bodyhigh2" pos="0 0 1.5" quat="0 0 0.382683 0 0.923876"/> -->
    <!-- <camera name="bodyhigh2" pos="0 0 1.5" quat="0 0.923876 0 0.382683"/> -->
